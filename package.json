    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-each": "^1.1.0",
    "postcss-each-variables": "^0.3.0",
    "postcss-for": "^2.1.1",
    "prettier": "^3.0.3",
    "typescript": "~5.1.6",
    "vite": "^4.4.9",
    "vitepress": "1.0.0-rc.20",
    "vitest": "^0.34.4",
