    "ant-design-vue": "^4.0.3",
    "axios": "^1.6.0",
    "element-plus": "^2.3.14",
    "pinia": "^2.1.6",
    "postcss": "^8.4.31",
    "postcss-color-mix": "^1.1.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
